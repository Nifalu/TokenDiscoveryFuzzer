{
  "fuzzer_preset": "preserving_tokens",
  "scheduler_preset": "fast",
  "silent_run": false,
  "disable_multimonitor": true,
  "timeout_secs": 8,

  "extractor": {"type": "corpus"},
  "pipeline": [
    {"type": "sais", "threshold_fn": {"type": "interpolated", "max_threshold": 0.025, "min_threshold": 0.015, "curve": 0.5}},
    {"type": "filter_null_bytes", "max_ratio": 0.4},
    {"type": "split_at", "delimiters": [[0, 0, 0]], "min_length": 3},
    {"type": "strip_bytes", "bytes": [0, 10, 13, 32], "min_length": 3},
    {"type": "remove_similar", "threshold": 0.2, "keep_longer": true},
    {"type": "remove_repetitive", "threshold": 0.8}
  ],

  "min_corpus_size": 300,
  "search_interval": 2000,
  "max_tokens": 1000,
  "max_token_length": 16,
  "min_token_length": 4,
  "search_pool_size": 5000,
  "displayed_tokens": 30,

  "cores": "10-12",
  "broker_port": 1340,
  "prometheus_host": "0.0.0.0",
  "prometheus_port": 8067,
  "corpus_dir": "./corpus",
  "crashes_dir": "./crashes_sais"
}